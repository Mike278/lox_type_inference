let zero_args_empty_body = \ { };
let zero_args_no_return = \ { print 1; };
let zero_args_with_return = \ { return 1; };
let zero_args_expr_body = \ -> 1;

let one_arg_empty_body = \a { };
let one_arg_no_return = \a { print a; };
let one_arg_with_return = \a { return a; };
let one_args_expr_body = \a -> a;

let two_args_empty_body = \a, b { };
let two_args_no_return = \a, b { print a; print b; };
let two_args_with_return = \a, b { return a + b; };
let two_args_expr_body = \a, b -> a + b;

print zero_args_empty_body();
print zero_args_no_return();
print zero_args_with_return();
print zero_args_expr_body();

print one_arg_empty_body(1);
print one_arg_no_return(1);
print one_arg_with_return(1);
print one_args_expr_body(1);

print two_args_empty_body(1, 2);
print two_args_no_return(1, 2);
print two_args_with_return(1, 2);
print two_args_expr_body(1, 2);

let assignment = "assignment!";
print assignment;

let is_great = \ {
    print "side effect!";
    return true;
};

print "=== false or is_great(); ===";
print false or is_great();

print "=== false and is_great(); ===";
print false and is_great();

print "=== true or is_great(); ===";
print true or is_great();

print "=== true and is_great(); ===";
print true and is_great();
