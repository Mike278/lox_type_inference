let map = \list, fn ->
    List.empty(list)
        ? []
        : [
            fn(List.first(list)),
            ..map(List.rest(list), fn)
          ];

let xs = [1, 2, 3];
print xs;

let ys = [];
print ys;

let zs = [1];
print zs;

let mixed = [1, {two: "three"}];
print mixed;



print map([1,2,3], \x -> x == 1);
print map(mixed, \x -> x == 1);
